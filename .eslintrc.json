{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "#require-jsdoc" : 0,
        "linebreak-style": 0,
        "new-cap": 0,
        "no-undef": "warn",
        "max-len": "off",
        "padded-blocks": "off",
        "no-invalid-this": "off"
    }
}